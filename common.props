<Project>
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <Version>5.2.2</Version>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>EasyAbp Team</Authors>
    <Description>ABP vNext framework Dapr module.</Description>
    <PackageProjectUrl>https://github.com/EasyAbp/EasyAbp.Abp.Dapr</PackageProjectUrl>
		<RepositoryUrl>https://github.com/EasyAbp/EasyAbp.Abp.Dapr</RepositoryUrl>
		<PackageTags>abp module easyabp dapr</PackageTags>
    <Company>EasyAbp</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SourceLink.Create.CommandLine" Version="2.8.3" PrivateAssets="All" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ConfigureAwait.Fody" Version="3.3.1" PrivateAssets="All" />
    <PackageReference Include="Fody" Version="6.2.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
